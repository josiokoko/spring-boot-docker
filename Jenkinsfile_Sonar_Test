pipeline {
  agent any
  tools {
    maven 'maven3.6.3'
    jdk 'jdk1.8'
  }
  
  stages {
  
    stage("Maven Build"){
      steps{
        sh 'mvn clean package'
      }
    }
    
    stage("Code Quality"){
      steps{
        sh 'mvn sonar:sonar'
      }
    }
    
    stage("Upload Artifact"){
      steps{
        sh 'mvn deploy'
      }
    }	
	  
  }
}
